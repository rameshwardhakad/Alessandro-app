{
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "buildjs": "vite build",
    "format": "prettier --write resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "format:check": "prettier --check resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .gitignore --fix resources/js",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .gitignore resources/js"
  },
  "dependencies": {
    "tailwindcss": "^3.4.3",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@stripe/stripe-js": "^3.4.1",
    "@tailwindcss/forms": "^0.5.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue-tsc": "^2.0.19"
  }
}
